local resourceBoards_GUID = {
    White  = '6888bc',
    Yellow = '75ff00',
    Red    = '598818',
    Green  = '07fb94',
    Blue   = 'd8348c',
    Purple = 'b50d53'
}

function getResourceBoard(color)
    -- Returns the resource board for the given color
    local resourceBoard = getObjectFromGUID(resourceBoards_GUID[color])
    log(resourceBoards_GUID[color], 'getResourceBoard:'..tostring(color))
    debugAssert(resourceBoard, 'No resource board found for '..tostring(color))

    return resourceBoard
end

function getNextCube(type, color)
    -- Returns the next unused population cube of a type of resource from a
    -- given Player color

    local resourceBoard = getResourceBoard(color)
    type = string.gsub(type, 'adv_', '')

    -- Start at position 1 and work around the track, finding the first unused
    -- population cube
    for i, cube in ipairs(resourceProduction[type]) do
        local n_pos = resourceBoard.positionToWorld(cube.position)
        local pop = castAndCheckForTag(n_pos, 'Population', 3)
        if pop then return pop end
    end
    log('No '..tostring(type)..' pop cube found for '..tostring(color), 'getNextCube')
    return nil
end

function placePopulations(hex, color, types)
    -- Fully fills a hex with populations

    typesToFill = types or {'money', 'materials', 'science'}

    for _, type in pairs(typesToFill) do
        for _, local_pos in pairs(hex.getVar(type)) do
            log("Type: "..tostring(type)..", Color: "..tostring(color), 'placePopulations')

            -- Find the next free cube
            local cube = getNextCube(type, color)
            debugAssert(cube ~= nil, "placePopulations: No population cube found.")

            f_pos = getRelativePosition(hex, local_pos)
            cube.setPositionSmooth(f_pos, false, true)
            cube.setRotation(hex.getRotation())
        end
    end
end

function placeStartingResources(color, race)
    -- Spawn a resource tile for a particular player, tag it and name it
    -- Sets the player's starting resources to the appropriate amount

    local faction = factions[race]
    debugAssert(faction ~= nil, "placeStartingResources: Did not find faction")

    -- Check if a replacement function is needed
    local replacement_func = faction.replacement_functions.placeStartingResources
    if replacement_func ~= nil then
        Global.call(replacement_func, {color= color, race= race})
        log('Replaced placeStartingResources with ['..replacement_func..']',
            'placeStartingResources')
        return
    end


    for _, resource in ipairs{"money", "materials", "science"} do
        local marker = spawnResourceMarker(resource, color)
        setPlayerResource(resource, color, faction[resource])
    end
end

function spawnResourceMarker(type, color)
    local marker
    if type == 'materials' then
        marker = getObjectFromGUID(materialsResourceMarker_GUID)
    elseif type == 'science' then
        marker = getObjectFromGUID(scienceResourceMarker_GUID)
    elseif type == 'money' then
        marker = getObjectFromGUID(moneyResourceMarker_GUID)
    else
        log('Error: Wrong type', 'spawnResourceMarker')
        return false
    end

    local newMarker = marker.clone()
    PlayerResourceMarkers[color][type]=newMarker
    newMarker.setLock(false)
    return newMarker
end

function setPlayerResource(resource, color, amount)
    -- Sets a player's stored amount of a given resource

    local resourceBoard = getResourceBoard(color)
    local marker = PlayerResourceMarkers[color][resource]
    local spot = Vector(resourceStorage[amount+1].position)

    local pos = resourceBoard.positionToWorld(spot) +Vector(0,2,0)

    log(spot, 'setPlayerResource: spot')
    log(pos, 'setPlayerResource: pos')

    debugAssert(pos ~= nil, "setPlayerResource: Pos is nil")
    debugAssert(resourceStorage[amount+1] ~= nil, "setPlayerResource: Storage is nil")

    -- Try and find the marker if it's missing
    if marker == nil then
        log('No resource marker found for '..color, 'setPlayerResource')
        local results = castBoxAndCheck(resourceBoard, resource..'ResourceMarker')
        if next(results) == nil then
            broadcastToColor(
                "Your "..resource.." marker is missing from your resource track.\nPlease return it to the track to enable automation.",
                color, Color.Red)
            return false
        else
            log('Spawning new marker '..color, 'setPlayerResource')
            marker = results[1]
        end
    end

    marker.setPositionSmooth(pos, false, true)
end
